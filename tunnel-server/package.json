{
  "name": "tunnel-server",
  "version": "1.0.0",
  "description": "Tunnel server for Voice-Controlled Terminal Management System",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "npm run dev",
    "start:prod": "npm run build && npm run start",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "lint": "echo 'No linter configured. Run type-check instead.' && npm run type-check",
    "test": "echo 'No tests configured yet'",
    "prestart": "npm run build"
  },
  "keywords": ["tunnel", "websocket", "proxy"],
  "author": "Roman Barinov <rbarinov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.8",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.2"
  }
}
