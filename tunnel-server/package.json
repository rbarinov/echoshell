{
  "name": "@rbarinov/tunnel-server",
  "version": "1.0.24",
  "description": "Tunnel server for Voice-Controlled Terminal Management System",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "npm run dev",
    "start:prod": "npm run build && npm run start",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "lint": "echo 'No linter configured. Run type-check instead.' && npm run type-check",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "prestart": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tunnel",
    "websocket",
    "proxy",
    "server",
    "relay",
    "bridge"
  ],
  "author": "Roman Barinov <rbarinov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rbarinov/echoshell.git",
    "directory": "tunnel-server"
  },
  "bugs": {
    "url": "https://github.com/rbarinov/echoshell/issues"
  },
  "homepage": "https://github.com/rbarinov/echoshell/tree/main/tunnel-server#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.8",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
